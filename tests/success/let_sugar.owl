locality alice

struct t { 
    _x : Data<adv> | |nonce| |,
    _y : Data<adv> | |nonce| |,
    _z : Data<adv> | |nonce| |
}

struct s {
    _a : Data<adv> | |nonce| |
}

def alice_main() @ alice : Unit =
    input i in
    parse i as t(x, y, z) in 
    let s' = s(x) in
    output s'
    otherwise ()
    
